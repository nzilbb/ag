    <ANNOTATION>
      <ALIGNABLE_ANNOTATION ANNOTATION_ID="a423" TIME_SLOT_REF1="ts423" TIME_SLOT_REF2="ts424">
        <ANNOTATION_VALUE>it with him. We will be happy to try to build consensus.</ANNOTATION_VALUE>
      </ALIGNABLE_ANNOTATION>
    </ANNOTATION>
