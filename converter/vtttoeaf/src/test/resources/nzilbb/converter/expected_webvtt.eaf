      <ALIGNABLE_ANNOTATION ANNOTATION_ID="a475" TIME_SLOT_REF1="ts475" TIME_SLOT_REF2="ts476">
        <ANNOTATION_VALUE>“It has come to the point where there is genuine surprise that the train is running</ANNOTATION_VALUE>
      </ALIGNABLE_ANNOTATION>
    </ANNOTATION>
    <ANNOTATION>
      <ALIGNABLE_ANNOTATION ANNOTATION_ID="a476" TIME_SLOT_REF1="ts476" TIME_SLOT_REF2="ts477">
        <ANNOTATION_VALUE>on time as opposed to it being so frequently cancelled ... The negative effects of this</ANNOTATION_VALUE>
      </ALIGNABLE_ANNOTATION>
    </ANNOTATION>
