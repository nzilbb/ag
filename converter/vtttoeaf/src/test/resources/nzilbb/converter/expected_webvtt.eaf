      <ALIGNABLE_ANNOTATION ANNOTATION_ID="a467" TIME_SLOT_REF1="ts467" TIME_SLOT_REF2="ts468">
        <ANNOTATION_VALUE>concerns on their constituents’ behalf.</ANNOTATION_VALUE>
      </ALIGNABLE_ANNOTATION>
    </ANNOTATION>
    <ANNOTATION>
      <ALIGNABLE_ANNOTATION ANNOTATION_ID="a468" TIME_SLOT_REF1="ts468" TIME_SLOT_REF2="ts469">
        <ANNOTATION_VALUE>We have used contractual mechanisms that are in the franchise agreement to require the</ANNOTATION_VALUE>
      </ALIGNABLE_ANNOTATION>
    </ANNOTATION>
    <ANNOTATION>
      <ALIGNABLE_ANNOTATION ANNOTATION_ID="a469" TIME_SLOT_REF1="ts469" TIME_SLOT_REF2="ts470">
