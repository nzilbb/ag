    </ANNOTATION>
    <ANNOTATION>
      <ALIGNABLE_ANNOTATION ANNOTATION_ID="a453" TIME_SLOT_REF1="ts453" TIME_SLOT_REF2="ts454">
        <ANNOTATION_VALUE>passenger confidence in the railway.</ANNOTATION_VALUE>
