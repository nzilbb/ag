    </ANNOTATION>
    <ANNOTATION>
      <ALIGNABLE_ANNOTATION ANNOTATION_ID="a455" TIME_SLOT_REF1="ts455" TIME_SLOT_REF2="ts456">
        <ANNOTATION_VALUE>was a breakdown, too—on the Borders railway on Sunday. The cancellations continued since</ANNOTATION_VALUE>
      </ALIGNABLE_ANNOTATION>
    </ANNOTATION>
    <ANNOTATION>
      <ALIGNABLE_ANNOTATION ANNOTATION_ID="a456" TIME_SLOT_REF1="ts456" TIME_SLOT_REF2="ts457">
        <ANNOTATION_VALUE>then and continue today.</ANNOTATION_VALUE>
      </ALIGNABLE_ANNOTATION>
    </ANNOTATION>
    <ANNOTATION>
      <ALIGNABLE_ANNOTATION ANNOTATION_ID="a457" TIME_SLOT_REF1="ts457" TIME_SLOT_REF2="ts458">
        <ANNOTATION_VALUE>I heard the Cabinet Secretary for Transport, Infrastructure and Connectivity talking this</ANNOTATION_VALUE>
      </ALIGNABLE_ANNOTATION>
    </ANNOTATION>
    <ANNOTATION>
      <ALIGNABLE_ANNOTATION ANNOTATION_ID="a458" TIME_SLOT_REF1="ts458" TIME_SLOT_REF2="ts459">
