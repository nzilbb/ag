        <ANNOTATION_VALUE>Rachael Hamilton (Ettrick, Roxburgh and Berwickshire) (Con):</ANNOTATION_VALUE>
      </ALIGNABLE_ANNOTATION>
    </ANNOTATION>
    <ANNOTATION>
      <ALIGNABLE_ANNOTATION ANNOTATION_ID="a474" TIME_SLOT_REF1="ts474" TIME_SLOT_REF2="ts475">
        <ANNOTATION_VALUE>I recently received a letter from a concerned Borders railway commuter. His letter says:</ANNOTATION_VALUE>
      </ALIGNABLE_ANNOTATION>
