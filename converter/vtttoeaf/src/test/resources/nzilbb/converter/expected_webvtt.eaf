    <ANNOTATION>
      <ALIGNABLE_ANNOTATION ANNOTATION_ID="a136" TIME_SLOT_REF1="ts136" TIME_SLOT_REF2="ts137">
        <ANNOTATION_VALUE>The number of consultants is up by 51 per cent; the number of qualified nurses and midwives</ANNOTATION_VALUE>
